<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".CompletedFragment">
    <TextView
        android:id="@+id/tv_Completed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Completed"
        app:layout_constraintStart_toEndOf="@id/button9"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/_23sdp"
        android:layout_marginStart="@dimen/_15sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="#E84C41"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_90sdp"
        android:text="4.5"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/_22sdp"
        android:layout_marginEnd="@dimen/_22sdp"
        android:drawableBottom="@drawable/ic_baseline_star_24"
        android:gravity="start"
        android:layout_marginStart="@dimen/_22sdp"
        android:textSize="@dimen/_22sdp"
        android:paddingTop="@dimen/_11sdp"
        android:paddingStart="@dimen/_18sdp"
        android:paddingBottom="@dimen/_22sdp"
        android:background="@drawable/btn"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_job"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:layout_marginTop="8dp"
        android:text="Carftsman Carpenter"
        android:textColor="@color/black"
        android:textSize="@dimen/_18sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.20"
        app:layout_constraintStart_toEndOf="@+id/button9"
        app:layout_constraintTop_toBottomOf="@+id/tv_Completed" />

    <TextView
        android:id="@+id/tv_Order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order"
        android:layout_marginTop="@dimen/_2sdp"
        android:textColor="#858282"
        android:textSize="@dimen/_22sdp"
        android:layout_marginStart="@dimen/_22sdp"
        app:layout_constraintStart_toEndOf="@+id/button9"
        app:layout_constraintTop_toBottomOf="@+id/tv_job" />

    <TextView
        android:id="@+id/tv_orderNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#52001"
        android:layout_marginTop="@dimen/_3sdp"
        android:textSize="@dimen/_22sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_Order"
        app:layout_constraintTop_toBottomOf="@+id/tv_job" />

    <TextView
        android:id="@+id/tv_Date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="28 NOV 2019"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/_2sdp"
        android:layout_marginStart="@dimen/_22sdp"
        android:textColor="@color/logo"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintBottom_toBottomOf="@+id/button9"
        app:layout_constraintStart_toEndOf="@+id/button9"
        app:layout_constraintTop_toBottomOf="@+id/tv_orderNumber" />

    <Button
        android:id="@+id/btn_details"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_30sdp"
        android:text="Details"
        android:textStyle="bold"
        android:textSize="@dimen/_10sdp"
        android:background="@drawable/btn_de"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/_22sdp"
        android:layout_marginStart="@dimen/_22sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_Date" />

    <TextView
        android:id="@+id/text_Detils"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_12sdp"
        android:layout_marginHorizontal="@dimen/_22sdp"
        android:layout_marginTop="@dimen/_22sdp"
        android:text="Hollwood isnt the only stateside destrination that
konws how to whip up a love story for the ages -we've handpiked a list of spots ..."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_details" />

    <TextView
        android:id="@+id/tv_ReadMore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RAED MORE..."
        android:textStyle="bold"
        android:textSize="@dimen/_11sdp"
        android:textColor="#D18008"
        android:layout_marginTop="@dimen/_6sdp"
        android:layout_marginStart="@dimen/_26sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Detils" />

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_22sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_ReadMore"
        app:tabSelectedTextColor="#D18008"
        app:tabTextColor="#D18008">

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="Caaftman Details" />


    </com.google.android.material.tabs.TabLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/asset"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabLayout2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="NO ANY PROVIDER RIGHT NOW"
        android:textColor="@color/red"
        android:layout_marginTop="@dimen/_22sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/_22sdp"
        android:layout_marginBottom="@dimen/_22sdp"
        android:background="@drawable/btn_de"
        android:text="RATE PROVIDER"
        android:textColor="@color/white"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>